x= input('Eres chico o chica?')
w='Bienvenid'
if x== 'chico':
    w+= 'o'
else:
    w += 'a'
    print(w,'al mundo de los pokemon!')
    
    y= input ('que edad tienes?') 
    if int(y)<10:
        if x == "chico":
            print('no tienes edad para ser entrenasdor')
        else:
            print('no tienes edad para ser entrenadora')
            quit()
            
    